			        <LinearLayout 
			            android:id="@+id/llo3_body2_llo2"
				        android:layout_width="fill_parent"
				        android:layout_height="fill_parent"
				        android:layout_weight="1"
				        android:paddingTop="3dp"
				        android:paddingBottom="3dp" >
